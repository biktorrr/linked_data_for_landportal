Lp:hasPostTypeName a rdf:Property ;
 rdfs:domain Lp:PostType;
 rdfs:label "has Post Type Name"@en ;
 rdfs:range rdfs:Literal.

Lp:hasPostYear a rdf:Property ;
 rdfs:domain Lp:PostType;
 rdfs:label "has Post Year"@en ;
